{
  "name": "genshin-material",
  "private": true,
  "type": "module",
  "scripts": {
    "lint": "eslint --ext .ts,.vue .",
    "test": "playwright-core install && vitest",
    "test:dev": "VITEST_DEV=true vitest",
    "dev": "nuxt dev --port 3002",
    "dev:host": "yarn dev --host",
    "dev:native": "NATIVE_UI=true yarn dev",
    "dev:native:host": "NATIVE_UI=true yarn dev --host",
    "run:ios": "NODE_ENV=development cap run ios",
    "run:android": "NODE_ENV=development cap run android",
    "deploy:ios": "cd ios/App && bundle exec fastlane deploy",
    "deploy:android": "cd android && bundle exec fastlane deploy",
    "sync:ios": "cap sync ios",
    "sync:android": "cap sync android",
    "generate": "nuxt generate",
    "generate:native": "NATIVE_UI=true nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.6.0",
    "@nuxt/devtools": "^1.0.8",
    "@nuxt/test-utils": "^3.10.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@types/eslint": "^8.56.2",
    "@types/luxon": "^3.4.2",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@vue/language-server": "^1.8.27",
    "@vue/test-utils": "^2.4.4",
    "bootstrap": "^5.3.2",
    "eslint": "^8.56.0",
    "eslint-config-nuxt": "github:chika3742/eslint-config-nuxt",
    "eslint-plugin-vue": "^9.20.1",
    "execa": "^8.0.1",
    "get-port-please": "^3.1.2",
    "happy-dom": "^13.3.1",
    "playwright-core": "^1.41.1",
    "vitest": "^1.2.2"
  },
  "dependencies": {
    "@capacitor/android": "^5.6.0",
    "@capacitor/app": "^5.0.6",
    "@capacitor/core": "^5.6.0",
    "@capacitor/ios": "^5.6.0",
    "@capacitor/splash-screen": "^5.0.6",
    "@chika3742/mhy-material-components": "^1.0.31",
    "@nuxtjs/google-fonts": "^3.1.3",
    "@nuxtjs/i18n": "^8.0.0",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.0",
    "@pinia/nuxt": "^0.5.1",
    "@rollup/plugin-dsv": "^3.0.4",
    "@rollup/plugin-yaml": "^4.1.2",
    "json-schema-to-typescript": "^13.1.2",
    "luxon": "^3.4.4",
    "nuxt": "^3.9.3",
    "sitemap": "^7.1.1",
    "tinygesture": "^3.0.0",
    "typescript": "^5.3.3",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5",
    "workbox-build": "^7.0.0",
    "yaml": "^2.3.4"
  },
  "packageManager": "yarn@4.0.2"
}
