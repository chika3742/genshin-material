{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:base"
  ],
  "branchConcurrentLimit": 0,
  "dependencyDashboard": false,
  "timezone": "Asia/Tokyo",
  "ignoreDeps": [
    "@chika3742/mhy-material-components"
  ],
  "hostRules": [
    {
      "matchHost": "https://npm.pkg.github.com/",
      "hostType": "npm",
      "encrypted": {
        "token": ""
      }
    }
  ],
  "branchPrefix": "deps-pages-nodeploy/",
  "packageRules": [
    {
      "matchFileNames": [
        "nuxt/package.json"
      ],
      "matchDepTypes": [
        "dependencies"
      ],
      "branchPrefix": "deps/",
      "labels": [
        "deps:nuxt"
      ],
      "semanticCommitScope": "nuxt"
    },
    {
      "matchFileNames": [
        "nuxt/package.json"
      ],
      "matchDepTypes": [
        "devDependencies"
      ],
      "labels": [
        "deps:nuxt"
      ],
      "semanticCommitScope": "nuxt"
    },
    {
      "matchFileNames": [
        "nuxt/ios/**/*"
      ],
      "labels": [
        "deps:ios"
      ],
      "semanticCommitScope": "ios"
    },
    {
      "matchFileNames": [
        "nuxt/android/**/*"
      ],
      "labels": [
        "deps:android"
      ],
      "semanticCommitScope": "android"
    },
    {
      "matchFileNames": [
        ".github/workflows/*"
      ],
      "labels": [
        "deps:workflow"
      ],
      "semanticCommitScope": "workflow"
    }
  ]
}
